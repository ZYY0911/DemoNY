<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#fff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/s_head2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_centerVertical="true"
                android:layout_marginLeft="150dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="时间上限:"
                    android:textSize="35dp"
                    android:textColor="#000"
                    />
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="160dp"
                    android:layout_width="180dp"
                    android:layout_height="50dp"
                    android:textSize="20dp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:id="@+id/start1"
                    android:background="@drawable/s_bk1"
                    />
                <ImageView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:src="@drawable/j2"
                    android:layout_marginLeft="320dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_marginLeft="600dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="时间下限："
                    android:textSize="35dp"
                    android:textColor="#000"
                    />

                <TextView
                    android:id="@+id/end1"
                    android:layout_width="180dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="170dp"
                    android:background="@drawable/s_bk1"
                    android:gravity="center"
                    android:textColor="#000"
                    android:textSize="20dp" />
                <ImageView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:src="@drawable/j2"
                    android:layout_marginLeft="330dp"
                    />
            </RelativeLayout>
            <Button
                android:layout_centerVertical="true"
                android:layout_marginLeft="1050dp"
                android:layout_width="120dp"
                android:layout_height="50dp"
                android:text="清空"
                android:id="@+id/qk"
                android:background="@drawable/s_bk2"
                />
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.7"
            android:padding="50dp"
            android:layout_height="match_parent">
            <LinearLayout
                android:background="@drawable/jlbk1"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ListView
                    android:divider="@null"
                    android:layout_width="match_parent"
                    android:id="@+id/listView"
                    android:layout_height="match_parent">

                </ListView>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:padding="50dp"
            android:layout_height="match_parent">
            <LinearLayout
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:background="@drawable/jlbk2"
                android:layout_height="match_parent">
                <ListView
                    android:layout_width="700dp"
                    android:layout_marginTop="20dp"
                    android:id="@+id/listVie1"
                    android:layout_height="match_parent">

                </ListView>
                <ExpandableListView
                    android:layout_marginTop="20dp"
                    android:id="@+id/ex"
                    android:visibility="gone"
                    android:layout_width="700dp"
                    android:layout_height="match_parent">
                </ExpandableListView>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>